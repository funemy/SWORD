package edu.tamu.aser.tide.trace;

public class ReadNode extends MemNode{
	
	public ReadNode(int GID, int TID, String addr, String sig, int line)
	{
		super(GID,TID, addr, sig, line);
	}
	public String getAddress()
	{
		return addr;
	}
	public int getTID() {
		return TID;
	}
	public int getGID() {
		return GID;
	}
	public String getSig()
	{
		return sig;
	}
	public int getLine()
	{
		return line;
	}
	
	public String toString()
	{
		return GID +" "+TID+" read "+addr +" "+ sig+" "+line;
	}
}
